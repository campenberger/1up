{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "space-before-function-paren": ["off"],
        "indent": ["warn", 4],
        "quote-props": ["off"],
        "camelcase": ["off"]
    }
}
